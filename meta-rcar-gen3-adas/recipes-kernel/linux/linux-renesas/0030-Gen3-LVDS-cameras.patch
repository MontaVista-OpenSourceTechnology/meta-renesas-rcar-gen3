+static const struct of_device_id ov2775_of_ids[] = {
+	{ .compatible = "ovti,ov2775", },
+	{ }
+};
+MODULE_DEVICE_TABLE(of, ov2775_of_ids);
+
+static struct i2c_driver ov2775_i2c_driver = {
+	.driver	= {
+		.name		= "ov2775",
+		.of_match_table	= ov2775_of_ids,
+	},
+	.probe		= ov2775_probe,
+	.remove		= ov2775_remove,
+	.id_table	= ov2775_id,
+};
+
