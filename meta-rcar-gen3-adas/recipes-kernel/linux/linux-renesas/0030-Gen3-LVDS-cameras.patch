+static __init int ov495_force_conf_link(char *str)
+{
+	/* force configuration link */
+	/* used only if robust firmware flashing required (f.e. recovery) */
+	force_conf_link = 1;
+	return 0;
+}
+early_param("force_conf_link", ov495_force_conf_link);
